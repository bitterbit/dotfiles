set term=screen-256color

" mandatory defaults
set nocompatible
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

" our plugins
Plugin 'gmarik/Vundle.vim'       
Plugin 'flazz/vim-colorschemes' 
Plugin 'altercation/vim-colors-solarized'

" colors
syntax enable
set background=dark
" colorscheme solarized


